<h1>Login</h1>

<%= form_with(url: login_path,scope: :session,local: true) do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br><br>
  <%= submit_tag "Login" %>
  <p>New user?<%=link_to"Sign up now!", new_user_path%></p>
<% end %>
